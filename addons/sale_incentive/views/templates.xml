                <div class="container">
                    <div class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-lg-8">
                                    <h4>
                                        <t>
                                            Sale Target :
                                        </t>
                                        <span t-esc="order.name"/>
                                    </h4>
                                </div>
                                <div class="col-lg-4">
                                    <h5>
                                        State:
                                    </h5>
                                    <t t-if="order.state=='request_related_dh_approve'">
                                        <span>Requested Related DH Approve</span>
                                    </t>
                                    <t t-if="order.state=='request_gm_approve'">
                                        <span>Requested GM Approve</span>
                                    </t>
                                    <t t-if="order.state=='request_coo_approve'">
                                        <span>Requested COO Approve</span>
                                    </t>
                                    <t t-if="order.state=='request_ceo_approve'">
                                        <span>Requested CEO Approve</span>
                                    </t>
                                </div>
                            </div>
                        </div>
                        <div class="card-body">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <strong>Branches:</strong>
                                        </div>
                                        <div class="col-lg-8">
                                            <span t-field="order.branch_ids"/>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <strong>Start Date:</strong>
                                        </div>
                                        <div class="col-lg-8">
                                            <span t-field="order.start_date" t-options='{"widget": "date"}'/>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            <br/>
                            <div class="row">


                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <strong>End Date:</strong>
                                        </div>
                                        <div class="col-lg-8">
                                            <span t-field="order.end_date"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <strong>B2B/B2C:</strong>
                                        </div>
                                        <div class="col-lg-8">
                                            <span t-field="order.b2b_b2c"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <br/>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <strong>Define BU Name:</strong>
                                        </div>
                                        <div class="col-lg-8">
                                            <span t-field="order.define_branch_id"/>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-6">
                                    <div class="row">
                                        <!-- <div class="col-lg-4">
                                          <strong>Target:</strong>
                                        </div>
                                         <div class="col-lg-8">
                                          <span t-field="order.target"/>
                                        </div> -->
                                    </div>
                                </div>
                            </div>

                            <br/>

                            <table class="table table-sm" id="sale_target_table">
                                <thread class="bg-100">
                                    <tr>
                                        <th class="text-left">Sale Team</th>
                                        <th class="text-right">Target</th>
                                    </tr>
                                </thread>
                                <tbody>
                                    <t t-foreach="order.sale_team_target_ids" t-as="sale_target">
                                        <tr>
                                            <td class="text-left">
                                                <span t-field="sale_target.sale_team_id"/>
                                            </td>
                                            <td class="text-right">
                                                <span t-field="sale_target.target"/>
                                            </td>
                                        </tr>
                                    </t>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div class="oe_structure mb32"/>
            </t>
        </template>

    </data>
