                        <button class="btn btn-primary mb-1 pt-1" data-target="#helpdesk_cash_close_modal"
                                data-toggle="modal">
                            <small>
                                <b>Approve</b>
                            </small>
                        </button>
                        <button class="btn btn-primary mb-1 pt-1" data-target="#reject_sale_modal" data-toggle="modal">
                            <small>
                                <b>
                                    Reject
                                </b>
                            </small>
                        </button>
