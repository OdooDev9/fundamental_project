<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_lot_serial_transfer_wizard" model="ir.ui.view">
        <field name="name">lot.serial.transfer.wizard.form</field>
        <field name="model">lot.serial.transfer.wizard</field>
        <field name="arch" type="xml">
            <form string="Check Lot Serail Number">
                <group>
                    <field name="message" readonly="1" nolabel="1"/>
                   
                </group>
                <footer>
                    <footer>
                    <button name="confirm" string="Confirm" type="object" class="btn-primary" data-hotkey="q"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" data-hotkey="z" />
                </footer>
                </footer>
            </form>
        </field>
    </record>
</odoo>