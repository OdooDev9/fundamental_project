<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<menuitem name="Configurations" id="ar_config_menu" parent="ar_assign_root_menu" sequence="20"/>
	<record model="ir.ui.view" id="assign_team_tree">
			<field name="name">AR Assign Team</field>
			<field name="model">assign.team</field>
			<field name="arch" type="xml">
				<tree>
					<field name="name" />
					<field name="employee_ids" widget="many2many_tags" />
					<field name="business_ids" widget="many2many_tags" />
					<field name="branch_ids" widget="many2many_tags" />
				</tree>
			</field>
		</record>
		<record model="ir.ui.view" id="assign_team_form">
			<field name="name">Assign Team</field>
			<field name="model">assign.team</field>
			<field name="arch" type="xml">
				<form string="Sales Team">
					<!-- <div class="alert alert-info text-center" role="alert" attrs="{'invisible': ['|', ('is_membership_multi', '=', True), ('member_warning', '=', False)]}">
						<field name="member_warning"/>
					</div> -->
					<sheet>
						<div class="oe_button_box" name="button_box"/>
						<!-- <widget name="web_ribbon" title="Archived" bg_color="bg-danger" attrs="{'invisible': [('active', '=', True)]}"/> -->
						<div class="oe_title">
							<label for="name" string="Assign Team"/>
							<h1>
								<field class="o_text_overflow" name="name"/>
							</h1>
							<!-- <div name="options_active"/> -->
						</div>
						<group>
							<group string="Team Details">
								<field name="employee_ids" widget="many2many_tags" />
								<field name="business_ids" widget="many2many_tags" />
								<field name="branch_ids" widget="many2many_tags" />

							</group>
							<group string="Due Type">
								<div >
									<span class="d-inline-block">
										<field name="over_due"/>
										<label for="over_due"/>
									</span>
									<span class="d-inline-block">
										<field name="future_due"/>
										<label for="future_due"/>
									</span>
									<span class="d-inline-block">
										<field name="current_due"/>
										<label for="current_due"/>
									</span>
								</div>
							</group>
						</group>
					</sheet>
					<div class="oe_chatter">
						<field name="message_follower_ids" help="Follow this assign to automatically track the events associated to users of this team."/>
						<field name="message_ids"/>
					</div>
				</form>
			</field>
		</record>
		<record model="ir.actions.act_window" id="assign_team_action_window">
			<field name="name">Assign Team</field>
			<field name="res_model">assign.team</field>
			<field name="view_mode">tree,form</field>
		</record>
		<menuitem name="Assign Team" id="assing_team_menu" parent="ar_config_menu" action="assign_team_action_window" sequence="2"/>
	<!-- End -->
