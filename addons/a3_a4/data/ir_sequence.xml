<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Sequence for A3A4 -->
        <record id="improvement_suggestion" model="ir.sequence">
            <field name="name">Improvement Suggestion Sequence</field>
            <field name="code">improvement.suggestion</field>
            <field name="prefix">IMP-SUG-%(range_year)s-%(month)s-</field>
            <field eval="True" name="use_date_range"/>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>