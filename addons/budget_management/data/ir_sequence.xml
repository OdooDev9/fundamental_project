<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Clearance (EX-CEI-YYYY-MM-#####) & Claim (AC-CEI-YYYY-MM-#####) number sequence -->

        <!-- Sequences for Clearance -->
        <record id="seq_budget_clearance" model="ir.sequence">
            <field name="name">Clearance</field>
            <field name="code">budget.clearance</field>
            <field name="prefix">-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Claim -->
        <record id="seq_budget_claim" model="ir.sequence">
            <field name="name">Clearance</field>
            <field name="code">budget.claim</field>
            <field name="prefix">-%(year)s-%(month)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>

