            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top" eval="25"/>
            <field name="margin_bottom" eval="38"/>
            <field name="margin_left" eval="2"/>
            <field name="margin_right" eval="2"/>
            <field name="header_line" eval="False"/>
            <field name="header_spacing" eval="27"/>
            <field name="dpi" eval="90"/>
            <field name="report_ids"
                   eval="[(4, ref('action_account_move_report'))]"/>
        </record>
