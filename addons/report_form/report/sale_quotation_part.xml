<odoo>
    <data>
        <template id="report_quotation_parts_header_id">
            <div class="header" style="width:100%;height:100px;font-size:20px;">
                <table style="width:100%;font-size:10px">
                    <tr style="width:100%">
                        <th style="width:30%;">
                            <img t-if="o.company_id.logo" t-att-src="image_data_uri(o.company_id.logo)"
                                 class="float-left" style="max-height: 100px;margin-left:-16px" alt="Logo"/>
                        </th>
                        <th style="width:40%;font-size:23px;text-align:center">
                            <span>
                                <strong>Quotation</strong>
                            </span>
                        </th>
                        <th style="width:30%;" class="text-right">
                            <img src='/report_form/static/description/images.png' style="max-height: 80px;"/>
                        </th>
                    </tr>
                </table>
                <br/>
                <!--                <div class="row" style="width:100%;text-align:center;">-->
                <!--                    <table style="width:100%">-->
                <!--                        <th style="width:10%;"/>-->
                <!--                        <th style="width:60%;text-align:center;font-size:15px">-->
                <!--                            <strong>Sale Order</strong>-->
                <!--                        </th>-->
                <!--                        <th style="width:10%;"/>-->
                <!--                    </table>-->
                <!--                </div>-->
                <!--                <br/>-->


            </div>

            <div class="footer o_standard_footer">
                <div class="last-page">
                    <table style="width:100%;text-align:center">
                        <tr style="width:100%">
                            <th style="text-align:left;width:33%;height:48px">
                                <span style="font-size:15px;padding-left:60px;">Prepared By</span>
                            </th>

                            <th style="text-align:center;width:33%">
                                <span style="font-size:15px;">Acknowledge By</span>
                            </th>

                            <th style="text-align:right;width:33%">
                                <span style="font-size:15px;padding-right:78px">Approved By</span>
                            </th>

                        </tr>
                        <tr style="width:100%">
                            <th style="text-align:left;width:33%">
                                <span style="font-size:15px;padding-right:17px">Signature</span>
                                <span>:</span>
                                <span>---------------------</span>
                            </th>
                            <th style="text-align:center;width:33%;margin-right:50px">
                                <span style="font-size:15px;padding-right:16px">Signature</span>
                                <span>:</span>
                                <span>---------------------</span>
                            </th>
                            <th style="text-align:center;width:33%;margin-right:50px">
                                <span style="font-size:15px;padding-right:16px">Signature</span>
                                <span>:</span>
                                <span>---------------------</span>
                            </th>
                        </tr>
                        <tr style="width:100%">
                            <th style="text-align:left;width:33%">
                                <span style="font-size:15px;padding-right:41px">Name</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                            <th style="text-align:center;width:33%;margin-right:30px">
                                <span style="font-size:15px;padding-right:39px">Name</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                            <th style="text-align:center;width:33%;margin-right:30px">
                                <span style="font-size:15px;padding-right:39px">Name</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                        </tr>
                        <tr style="width:100%">
                            <th style="text-align:left;width:33%">
                                <span style="font-size:15px;">Position</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                            <th style="text-align:center;width:33%;">
                                <span style="font-size:15px;">Position</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                            <th style="text-align:center;width:33%;">
                                <span style="font-size:15px;">Position</span>
                                <span>:</span>
                                <span>---------------------</span>

                            </th>
                        </tr>
                    </table>
                </div>
                <div class="text-muted" style="width:100%;text-align:center;">
                    <div class="col-md-12" style="width:100%;text-align:center">
                        <strong>No-589/592 Bo Aung Kyaw Street, Yangon-Pathein Highway Road</strong>
                        <br/>
                        <strong>Tel: +959690443, +959450043173, +959250745310, +959254903695</strong>
                    </div>
                </div>
            </div>
        </template>
        <template id="report_sale_quotation_parts_body">
            <t t-call="web.external_layout">
                <t t-set="o" t-value="o.with_context(lang=o.partner_id.lang)"/>
                <div class="page"
                     style="font-family: Calibri, Candara, Segoe, Segoe UI, Optima, Arial, sans-serif, Myanmar3;">
                    <div class="row" style="width:100%">
                        <div class="col-md-6" style="width:50%;font-size:14px;">
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Customer</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left;">
                                    <span t-esc="o.partner_id.name"/>
                                </div>
                            </div>
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Contact Person</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left">
                                    <span t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M')"/>
                                </div>
                            </div>
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Contact Phone</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left">
                                    <!--                                    <span t-esc="o.req_quot_id.name"/>-->
                                </div>
                            </div>
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Machine Information</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left">
                                    <t t-foreach="o.req_quotation_line" t-as="line">
                                        <span t-esc="line.product_id.machine_info"/>
                                    </t>
                                </div>
                            </div>
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Currency</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left">
                                    <span t-esc="o.pricelist_id.currency_id.name"/>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6" style="width:60%;font-size:14px;padding-left:1px">
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:20%">
                                    <strong>Quo No.</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:35%;text-align:left">
                                    <span t-esc="o.name"/>
                                </div>
                            </div>

                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:20%">
                                    <strong>Quo Date</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:35%;text-align:left">
                                    <span t-esc="o.date_order"/>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br/>
                    <br/>
                    <br/>
                    <table class="table table-sm o_main_table" style="margin-top:-48px">
                        <thead>
                            <tr style="width:100%;font-size:14px">
                                <th name="th_no" style="width:5%;border:1px solid black;color:black"
                                    class="text-center">
                                    <strong>No</strong>
                                </th>
                                <th style="color:black;width:30%;border:1px solid black" class="text-center">
                                    <strong>Brand</strong>
                                </th>
                                <th class="text-center" style="color:black;width:10%;border:1px solid black">
                                    <strong>Product</strong>
                                </th>
                                <th class="text-center" style="color:black;width:3%;border:1px solid black">
                                    <strong>Description</strong>
                                </th>
                                <th class="text-center" style="color:black;width:5%;border:1px solid black">
                                    <span>QTY</span>
                                </th>
                                <th class="text-center" style="color:black;width:5%;border:1px solid black">
                                    <span>UOM</span>
                                </th>
                                <th class="text-center" style="color:black;width:10%;border:1px solid black">
                                    <span>Date</span>
                                </th>
                                <th class="text-center" style="color:black;width:20%;border:1px solid black">
                                    <strong>Price</strong>
                                </th>
                                <th class="text-center" style="color:black;width:20%;border:1px solid black">
                                    <strong>Amount</strong>
                                </th>
                            </tr>
                        </thead>
                        <tbody style="font-size:14px">
                            <t t-set="current_subtotal" t-value="0"/>
                            <t t-set="no" t-value="0"/>
                            <tr t-foreach="o.req_quotation_line" t-as="line">
                                <t t-set="no" t-value="no+1"/>
                                <td style="text-align:center;color:black;width:5%;border:1px solid black">
                                    <span t-esc="no"/>
                                </td>
                                <td style="width:30%;border:1px solid black" class="text-left">
                                    <span t-esc="line.product_id.brand.name"/>
                                </td>
                                <td class="text-center" style="width:10%;border:1px solid black">
                                    <span t-esc="line.product_id.name"/>
                                </td>
                                <td class="text-center" style="width:3%;border:1px solid black">
                                    <!--                                    <span t-esc="'%.0f' % line.product_uom_qty"/>-->
                                    <span t-esc="line.name"/>
                                </td>
                                <td class="text-center" style="width:5%;border:1px solid black">
                                    <span t-esc="line.product_uom_qty"/>
                                </td>
                                <td t-attf-class="text-center" style="width:10%;border:1px solid black">
                                    <!--                                    <span t-esc="line.date.strftime('%d-%m-%Y')"/>-->
                                    <span t-esc="line.product_uom.name"/>
                                </td>
                                <td t-attf-class="text-center" style="width:10%;border:1px solid black">
                                    <!--                                    <span t-esc="line.date.strftime('%d-%m-%Y')"/>-->
                                    <span t-esc="line.estimated_delivery"/>
                                </td>
                                <td t-attf-class="text-center" style="width:10%;border:1px solid black">
                                    <!--                                    <span t-esc="line.date.strftime('%d-%m-%Y')"/>-->
                                    <span t-esc="line.price_unit"/>
                                </td>
                                <td t-attf-class="text-center" style="width:10%;border:1px solid black">
                                    <span t-esc="line.price_subtotal"/>
                                    <!--                                    <span t-esc="line.price_unit"/>-->
                                </td>
                                <!--                                <td t-attf-class="text-center {{ 'd-none d-md-table-cell' if report_type == 'html' else '' }}"-->
                                <!--                                    style="width:20%;border:1px solid black">-->
                                <!--                                    <span t-esc="line.remark"/>-->
                                <!--                                </td>-->

                            </tr>
                        </tbody>
                    </table>
                    <table class="total" style="width:100%">
                        <tr>
                            <th style="width:5%;"/>
                            <th style="width:30%"/>
                            <th style="width:10%;"/>
                            <th style="width:15%;"/>
                            <th style="width:15%;"/>
                            <th style="width:10%;text-align:right;margin-right:-80px;">Subtotal :</th>
                            <th style="width:13%;text-align:right;">
                                <span style="padding-left:20px" t-esc="o.amount_untaxed"/>
                                <!--                            <span t-esc="o.currency_id.symbol"/>-->
                            </th>
                        </tr>
                        <tr t-if="o.amount_tax">
                            <th style="width:5%"/>
                            <th style="width:30%"/>
                            <th style="width:10%"/>
                            <th style="width:15%"/>
                            <th style="width:15%"/>
                            <th style="width:12%;text-align:right">Tax :</th>
                            <th style="width:13%;text-align:right">
                                <span t-esc="o.amount_tax"/>
                                <!--                            <span t-esc="o.currency_id.symbol"/>-->
                            </th>
                        </tr>
                        <tr t-if="o.discount_value">
                            <th style="width:5%"/>
                            <th style="width:30%"/>
                            <th style="width:10%"/>
                            <th style="width:15%"/>
                            <th style="width:15%"/>
                            <th style="width:12%;text-align:right;">Discount :</th>
                            <th style="width:13%;text-align:right">
                                <span t-esc="'{0:,.2f}'.format(o.discount_value)"/>
                                <!--                            <span t-esc="o.currency_id.symbol"/>-->
                            </th>
                        </tr>
                        <tr t-if="o.amount_total">
                            <th style="width:5%"/>
                            <th style="width:30%"/>
                            <th style="width:10%"/>
                            <th style="width:15%"/>
                            <th style="width:15%"/>
                            <th style="width:12%;text-align:right;">
                                Total :
                            </th>
                            <th style="width:13%;text-align:right;">
                                <span t-esc="o.amount_total"/>
                                <!--                            <span t-esc="o.currency_id.symbol"/>-->
                            </th>
                        </tr>
                    </table>
                    <div class="oe_structure"/>
                    <div class="row" style="width:100%">
                        <div class="col-md-6" style="width:50%;font-size:14px;">
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Delivery Date</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left;">
                                    <!--                                    <span t-esc="o.commitment_date.strftime('%d-%m-%Y %I:%M:%S %p')"/>-->
                                </div>
                            </div>
                            <div class="row" style="width:100%">
                                <div class="col-md-5" style="width:40%">
                                    <strong>Payment Terms</strong>
                                </div>
                                <div class="col-md-1" style="width:1%">
                                    <strong>:</strong>
                                </div>
                                <div class="col-md-6" style="width:54%;text-align:left">
                                    <span t-foreach="o.payment_term_id.line_ids" t-as="p">
                                        <span t-esc="p.value"/>
                                        <br/>
                                        <span t-esc="p.value_amount"/>
                                        <br/>
                                        <span t-esc="p.days"/>-
                                        <span t-esc="p.option"/>
                                    </span>
                                </div>
                            </div>
                            <t t-if="o.note">
                                <div class="row" style="width:100%">
                                    <div class="col-md-5" style="width:40%">
                                        <strong>Remark</strong>
                                    </div>
                                    <div class="col-md-1" style="width:1%">
                                        <strong>:</strong>
                                    </div>
                                    <div class="col-md-6" style="width:54%;text-align:left">
                                        <span t-esc="o.note"/>
                                    </div>
                                </div>
                            </t>
                        </div>
                    </div>
                </div>
            </t>
        </template>
        <template id="report_sale_quotation_parts_document">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="report_form.report_quotation_parts_header_id" t-lang="o.partner_id.lang"/>
                    <t t-call="report_form.report_sale_quotation_parts_body" t-lang="o.partner_id.lang"/>
                </t>
            </t>
        </template>
    </data>
</odoo>